    case o.pay_type when 1 then '微信' when 2 then '支付宝' else '其他' end as repay_type,
    tc.cust_code,
    tc.cust_name,
    tc.company_name
    from
    t_cust_deposit_order o
    left join t_cust_account a on o.cust_id = a.cust_id
    left join t_customer tc on o.cust_id = tc.cust_id
    <where>
      <include refid="queryPageList_where" />
    </where>
  </select>

  <!-- 客户充值记录查看-->
